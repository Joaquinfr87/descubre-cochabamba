generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model ContactSubmission {
  id        Int      @id @default(autoincrement())
  email     String
  name      String   @default("Anonymous")
  message   String
  createdAt DateTime @default(now())
}

model Review {
  id        Int      @id @default(autoincrement())
  author    String
  rating    Int
  content   String
  createdAt DateTime @default(now())
}
